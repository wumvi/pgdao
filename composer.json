{
  "name": "wumvi/pgdao",
  "authors": [
    {
      "name": "Vitaliy Kozlenko",
      "email": "vk@wumvi.com"
    }
  ],
  "require": {
    "ext-pgsql": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.2.2",
    "squizlabs/php_codesniffer": "3.5.5",
    "overtrue/phplint": "2.0.2",
    "phpstan/phpstan": "0.12.28",
    "phan/phan": "3.0.2"
  },
  "autoload": {
    "psr-4": {
      "Wumvi\\PgDao\\": "src"
    }
  },
  "scripts": {
    "phpstan": [
      "phpstan analyze --level=7 src -c phpstan.neon"
    ],
    "phplint": [
      "phplint"
    ],
    "phpunit": [
      "phpunit"
    ],
    "phpcs": [
      "phpcs src --parallel=10 --runtime-set php_version 74000 -p --standard=ruleset.xml"
    ],
    "test": [
      "@phpstan",
      "@phplint",
      "@phpunit",
      "@phpcs"
    ]
  },
  "license": "proprietary"
}
